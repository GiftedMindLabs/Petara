{
  "expo": {
    "name": "Petara",
    "slug": "Petara",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo01.png",
    "scheme": "petara",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.giftedmindlabs.Petara",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"],
        "NSUserNotificationAlertUsageDescription": "Allow notifications to stay updated about your pet's needs."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foreground.png",
        "backgroundColor": "#18968A"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.giftedmindlabs.Petara"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/foreground.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#18968A"
        }
      ],
      "expo-sqlite",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "85af9d15-60d8-4b5b-b2ad-8452200ae381"
      }
    }
  }
}
